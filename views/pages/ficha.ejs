<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>
        <%= persona.nombre %> / Landing | landing.com</title>
    <!-- Bootstrap Core CSS -->
    <link href="../vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!-- MetisMenu CSS -->
    <link href="../vendor/metisMenu/metisMenu.min.css" rel="stylesheet">
    <!-- Material CSS -->
    <link href="../dist/css/mdb.min.css" rel="stylesheet">
    <!-- Custom CSS -->
    <link href="../dist/css/sb-admin-2.css" rel="stylesheet">
    <link href="../dist/css/star-rating.css" rel="stylesheet">
    <!-- Custom Fonts -->
    <link href="../vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Titillium+Web:400,700italic,600italic,400italic,300italic,200italic,900,700,600,300,200"
        rel="stylesheet" type="text/css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
      <![endif]-->
    <style>
        .gl-star-rating {
            margin-bottom: 1em;
        }

        pre {
            hyphens: none;
            tab-size: 4;
            white-space: pre;
            word-break: normal;
            word-spacing: normal;
            word-wrap: normal;
        }

        header ul a {
            line-height: 1.2;
            padding-top: 5px;
        }
    </style>
    <!-- Google Tag Manager -->
    <script>(function (w, d, s, l, i) {
        w[l] = w[l] || []; w[l].push({
            'gtm.start':
                new Date().getTime(), event: 'gtm.js'
        }); var f = d.getElementsByTagName(s)[0],
            j = d.createElement(s), dl = l != 'dataLayer' ? '&l=' + l : ''; j.async = true; j.src =
                'https://www.googletagmanager.com/gtm.js?id=' + i + dl; f.parentNode.insertBefore(j, f);
        })(window, document, 'script', 'dataLayer', 'GTM-xxxxxxx');
    </script>
    <!-- End Google Tag Manager -->

</head>

<body class="fixed-sn black-skin">
    <header>
        <% include ../partials/navbar %>
    </header>
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="content-banner banner970x90">

        </div>
    </div>
    <main>
        <div id="wrapper">
            <div id="page-wrapper" class="container-fluid" itemscope itemtype="http://schema.org/Organization">
                <div class="row">
                    <div class="col-lg-8 col-md-12 col-sm-12 col-xs-12">
                        <h1 class="page-header list-group-item active" itemprop="name">
                            <%= persona.datosEmpresa.nombreEmpresa %>
                        </h1>
                        <div class="panel panel-primary">
                            <div class="panel-heading" itemprop="member" itemscope itemtype="http://schema.org/OrganizationRole">
                                <div class="clearfix">
                                    <div class="pull-left img-perfil">
                                        <img style="max-width:150px;max-height:150px" src="/logos/dummypersona.jpg">
                                    </div>
                                    <div class="pull-left data-perfil">
                                        <h2 class="landing" itemprop="member" itemscope itemtype="http://schema.org/Person">
                                            <span itemprop="name">
                                                <%= persona.nombre %>
                                            </span>
                                        </h2>
                                        <h4 class="cargoQuien" itemprop="roleName">
                                            <%= persona.cargo %>
                                        </h4>
                                        <select id="star-rating" class="col-xs-6">
                                            <option value="">Puntaje</option>
                                            <option value="5">Excelente</option>
                                            <option value="4">Muy bueno</option>
                                            <option value="3">Promedio</option>
                                            <option value="2">Pobre</option>
                                            <option value="1">Terrible</option>
                                        </select>
                                    </div>
                                </div>
                                <div>
                                    <%if (!islogin) { %>
                                        <%- include('../partials/blockcontent', {empresa: persona.nombre})  %>
                                            <%} else { %>
                                                <ul class="list-group">
                                                    <% 
                                       var jsonDatosAMostrar = persona;
                                       delete jsonDatosAMostrar.empresaid;
                                       delete jsonDatosAMostrar._id;
                                       delete jsonDatosAMostrar.webID;
                                       delete jsonDatosAMostrar["menciones en publicaciones"];
                                       delete jsonDatosAMostrar.datosEmpresa;
                                       delete jsonDatosAMostrar.nombre;
                                       delete jsonDatosAMostrar.cargo;
                                       delete jsonDatosAMostrar.personaid;
                                       for (var key in jsonDatosAMostrar){
                                           if (jsonDatosAMostrar.hasOwnProperty(key)){ %>
                                                        <li class="list-group-item">
                                                            <span>
                                                                <strong>
                                                                    <%=key%>:</strong>
                                                            </span>
                                                            <span>
                                                                <%=jsonDatosAMostrar[key];%>
                                                            </span>
                                                        </li>
                                                        <%} %>
                                                            <% } %>
                                                </ul>
                                                <% } %>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="colum-nota" class="col-lg-4 col-md-12 col-sm-12 col-xs-12">
                        <!--CONTENEDOR DEL BANNER-->
                        <div class="content-banner banner300x250">

                        </div>
                    </div>
                    <div class="mod-it-cxense col-lg-8 col-md-12 col-sm-12 col-xs-12">
                        <div class="cxense-decoration">
                            <h2 class="title-mod-cxense-it"> Noticias de tu interes</h2>

                        </div>
                    </div>

                </div>
                <!-- /.row -->
            </div>
            <!-- /#page-wrapper -->
        </div>
    </main>
    <!-- /#wrapper -->
    <% include ../partials/commonscript %>
        <script>
            var starrating = new StarRating(document.getElementById('star-rating'));
        </script>
</body>

</html>